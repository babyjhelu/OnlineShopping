<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
          xmlns:o="urn:schemas-microsoft-com:office:office"
          xmlns:x="urn:schemas-microsoft-com:office:excel"
          xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
          xmlns:html="http://www.w3.org/TR/REC-html40">
  <Worksheet ss:Name="Sheet1">
    <Table>
      <Row>
        <Cell><Data ss:Type="String">ID</Data></Cell>
        <Cell><Data ss:Type="String">Category Name</Data></Cell>
        <Cell><Data ss:Type="String">Description</Data></Cell>
        <Cell><Data ss:Type="String">Image</Data></Cell>
        <Cell><Data ss:Type="String">Status</Data></Cell>
      </Row>
      <% @categories.each do |category| %>
          <Row>
            <Cell><Data ss:Type="Number"><%= category.id %></Data></Cell>
            <Cell><Data ss:Type="String"><%= category.category_name %></Data></Cell>
            <Cell><Data ss:Type="String"><%= category.description %></Data></Cell>
            <Cell><Data ss:Type="String"><%= category.image %></Data></Cell>
            <Cell><Data ss:Type="Status"><%= category.status %></Data></Cell>
          </Row>
      <% end %>
    </Table>
  </Worksheet>
</Workbook>